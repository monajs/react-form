  "description": "最简单的 React 表单组件",
  "scripts": {
    "start": "node ./build/webpack.dev.js",
    "build": "node ./build/webpack.build.js",
    "compile": "rm -rf lib && export BABEL_ENV=package && babel package --out-dir lib --copy-files",
